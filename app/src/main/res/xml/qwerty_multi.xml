<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >

    <Row>
        <Key android:codes="113" android:keyLabel="q"
		android:keyEdgeFlags="left"/>
        <Key android:codes="119" android:keyLabel="w"/>
        <Key android:codes="101" android:keyLabel="e"/>
        <Key android:codes="114" android:keyLabel="r"/>
        <Key android:codes="116" android:keyLabel="t"/>
        <Key android:codes="121" android:keyLabel="y"/>
        <Key android:codes="117" android:keyLabel="u"/>
        <Key android:codes="105" android:keyLabel="i"/>
        <Key android:codes="111" android:keyLabel="o"/>
        <Key android:codes="112" android:keyLabel="p"
		android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="97" android:keyLabel="a"
                android:keyEdgeFlags="left"/>
        <Key android:codes="115" android:keyLabel="s"/>
        <Key android:codes="100" android:keyLabel="d"/>
        <Key android:codes="102" android:keyLabel="f"/>
        <Key android:codes="103" android:keyLabel="g"/>
        <Key android:codes="104" android:keyLabel="h"/>
        <Key android:codes="106" android:keyLabel="j"/>
        <Key android:codes="107" android:keyLabel="k"/>
        <Key android:codes="108" android:keyLabel="l"/>
        <Key android:codes="8" android:keyLabel="BS"
                android:keyWidth="10%p"
                android:keyEdgeFlags="right"
                android:isRepeatable="true"/>
    </Row>
    
    <Row>
        <Key android:codes="27" android:keyLabel="Esc"
                android:keyWidth="10%p"
                android:keyEdgeFlags="left"/>
        <Key android:codes="122" android:keyLabel="z"/>
        <Key android:codes="120" android:keyLabel="x"/>
        <Key android:codes="99" android:keyLabel="c"/>
        <Key android:codes="118" android:keyLabel="v"/>
        <Key android:codes="98" android:keyLabel="b"/>
        <Key android:codes="110" android:keyLabel="n"/>
        <Key android:codes="109" android:keyLabel="m"/>
        <Key android:codes="64,35" android:keyLabel="\@" android:popupCharacters="\@#" android:popupKeyboard="@xml/popup"/>
        <Key android:codes="127" android:keyLabel="Del"
                android:keyWidth="10%p"
		android:isRepeatable="true"
		android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="-10" android:keyLabel="Ctrl"
                android:keyWidth="10%p"
		android:isSticky="true"
		android:keyEdgeFlags="left"/>
        <Key android:codes="33,63" android:keyLabel="!" android:popupCharacters="!\?" android:popupKeyboard="@xml/popup"/>
        <Key android:codes="36,37,38" android:keyLabel="$" android:popupCharacters="$%&amp;" android:popupKeyboard="@xml/popup"/>
        <Key android:codes="42,94,126" android:keyLabel="*" android:popupCharacters="*^~" android:popupKeyboard="@xml/popup"/>
        <Key android:codes="45,95,43" android:keyLabel="-" android:popupCharacters="-_+" android:popupKeyboard="@xml/popup"/>
        <Key android:codes="46,44" android:keyLabel="." android:popupCharacters=".," android:popupKeyboard="@xml/popup"/>
        <Key android:codes="58,59,61" android:keyLabel=":" android:popupCharacters=":;=" android:popupKeyboard="@xml/popup"/>
        <Key android:codes="47,39,34,96" android:keyLabel="/" android:popupCharacters="/\'&quot;`" android:popupKeyboard="@xml/popup"/>
        <Key android:codes="92,124" android:keyLabel="\\" android:popupCharacters="\\|" android:popupKeyboard="@xml/popup"/>
        <Key android:codes="9" android:keyLabel="Tab"
                android:keyWidth="10%p"
		android:isRepeatable="true"
		android:keyEdgeFlags="right"/>
    </Row>
    
    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="-2" android:keyLabel="123"
		android:keyWidth="10%p"
		android:keyEdgeFlags="left"/>
        <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift" 
                android:keyWidth="10%p"
		android:isModifier="true"
                android:isSticky="true"
		/>
        <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" 
                android:keyWidth="10%p"
		android:isRepeatable="true"/>
        <Key android:codes="91,60,40,123" android:keyLabel="[" android:popupCharacters="[&lt;({" android:popupKeyboard="@xml/popup"/>
        <Key android:codes="93,62,41,125" android:keyLabel="]" android:popupCharacters="]&gt;)}" android:popupKeyboard="@xml/popup"/>
        <Key android:codes="21" android:keyLabel="←"
                android:keyWidth="10%p"
		android:isRepeatable="true"/>
        <Key android:codes="20" android:keyLabel="↓"
                android:keyWidth="10%p"
		android:isRepeatable="true"/>
        <Key android:codes="19" android:keyLabel="↑"
                android:keyWidth="10%p"
		android:isRepeatable="true"/>
        <Key android:codes="22" android:keyLabel="→"
                android:keyWidth="10%p"
		android:isRepeatable="true"/>
        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" 
                android:keyWidth="10%p"
		android:isRepeatable="true"
		android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>
